{
    "name": "codesaur/indoraptor",
    "description": "An elegant object-oriented application development framework for PHP 7.2 or newer",
    "keywords": [
        "indoraptor", "application", "http application", "server request handler", "psr-3", "psr-7", "psr-15",
        "mongol", "mongolia", "mongolian", "монгол", "codesaur", "narankhuu", "coden", "кодзавр", "наранхүү", "коден",
        "ololt", "ололт", "мөнхийн", "мөнхийн-ололт", "мөнхийн ололт", "munkhiin ololt"
    ],
    "homepage": "https://ololt.mn/codesaur",
    "authors": [
        {
            "name": "Narankhuu",
            "email": "codesaur@gmail.com",
            "homepage": "https://ololt.mn/narankhuu"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "firebase/php-jwt": ">=5.2.0",
        "phpmailer/phpmailer": ">=6.0",
        "codesaur/http-application": ">=1.2",
        "codesaur/dataobject": ">=2.5",
        "codesaur/rbac": ">=1.4",
        "codesaur/localization": ">=1.3",
        "codesaur/logger": ">=1.1.1"
    },
    "autoload": {
        "psr-4": {
